<Window x:Class="Deliverable7.frmItem"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Deliverable7"
        mc:Ignorable="d"
        Title="Yay, an item!" Height="300" Width="300">
    <Grid>
        <TextBlock x:Name="txtFound" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
        <Button Name="btnNo" Content="No" HorizontalAlignment="Left" Margin="173,157,0,0" VerticalAlignment="Top" Width="75" Click="btnNo_Click"/>
        <Button Name="btnYes" Content="Yes" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="42,157,0,0" Click="btnYes_Click"/>
    </Grid>
</Window>
